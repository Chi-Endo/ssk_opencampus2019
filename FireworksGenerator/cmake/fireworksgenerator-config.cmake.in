# FireworksGenerator CMake config file
#
# This file sets the following variables:
# FireworksGenerator_FOUND - Always TRUE.
# FireworksGenerator_INCLUDE_DIRS - Directories containing the FireworksGenerator include files.
# FireworksGenerator_IDL_DIRS - Directories containing the FireworksGenerator IDL files.
# FireworksGenerator_LIBRARIES - Libraries needed to use FireworksGenerator.
# FireworksGenerator_DEFINITIONS - Compiler flags for FireworksGenerator.
# FireworksGenerator_VERSION - The version of FireworksGenerator found.
# FireworksGenerator_VERSION_MAJOR - The major version of FireworksGenerator found.
# FireworksGenerator_VERSION_MINOR - The minor version of FireworksGenerator found.
# FireworksGenerator_VERSION_REVISION - The revision version of FireworksGenerator found.
# FireworksGenerator_VERSION_CANDIDATE - The candidate version of FireworksGenerator found.

message(STATUS "Found FireworksGenerator-@PROJECT_VERSION@")
set(FireworksGenerator_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(FireworksGenerator_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(FireworksGenerator_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(FireworksGenerator_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(FireworksGenerator_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(FireworksGenerator_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(FireworksGenerator_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(FireworksGenerator_DEFINITIONS ${<dependency>_DEFINITIONS})

set(FireworksGenerator_VERSION @PROJECT_VERSION@)
set(FireworksGenerator_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(FireworksGenerator_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(FireworksGenerator_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(FireworksGenerator_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

